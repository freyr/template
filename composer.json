{
    "name": "freyr/template",
    "description": "application template",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "license": "proprietary",
    "require": {
        "php": "8.3.*",
        "ramsey/uuid": "^4.5",
        "symfony/console": "^7.0",
        "symfony/framework-bundle": "^7.0",
        "symfony/runtime": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5.13"
    },
    "autoload": {
        "psr-4": {
            "Freyr\\Template\\": "src/"
        }
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "process-timeout": 1500,
        "sort-packages": true,
        "allow-plugins": {
            "symfony/runtime": true
        }
    },
    "version": "1.0.1"
}
